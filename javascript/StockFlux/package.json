{
  "name": "StockFlux",
  "description": "Hosts BitFlux as an OpenFin application.",
  "version": "11.0.1",
  "private": true,
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/ScottLogic/StockFlux.git"
  },
  "scripts": {
    "start": "npm run clean && webpack-dev-server -d --config webpack/webpack.config.dev.js",
    "openfin:dev": "node openfin-launcher/dev.js",
    "openfin:prod": "node openfin-launcher/prod.js",
    "build": "npm run clean && webpack -p --config webpack/webpack.config.prod.js",
    "build:deploy": "npm run build && node openfin-config/updateForDeployment.js",
    "serve": "http-server -p 5001",
    "prod": "npm run build && npm run serve",
    "clean": "rimraf public/*",
    "ci": "npm run lint && npm test && npm run build",
    "lint": "eslint --ignore-path .gitignore -- .",
    "lint:fix": "eslint --fix --ignore-path .gitignore -- .",
    "test": "cross-env BABEL_ENV=test mocha --opts test/mocha.opts",
    "test:watch": "npm test -- --watch",
    "test:cov": "cross-env BABEL_ENV=test babel-node node_modules/isparta/bin/isparta cover --report text --report html node_modules/mocha/bin/_mocha -- --opts test/mocha.opts",
    "remotedev": "remotedev --hostname=localhost --port=8000",
    "release": "npm run ci && npm version"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-rewire": "^1.0.0-rc-3",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "6.26.0",
    "babel-template": "^6.9.0",
    "babel-types": "^6.26.0",
    "chai": "^4.1.2",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^1.0.8",
    "css-loader": "^0.28.1",
    "deep-freeze": "0.0.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^1.1.11",
    "grunt-cli": "^1.2.0",
    "http-server": "^0.11.1",
    "isparta": "^4.0.0",
    "jsdom": "9.3.0",
    "jsdom-global": "2.0.0",
    "json-loader": "^0.5.4",
    "less": "^3.0.1",
    "less-loader": "^4.1.0",
    "mocha": "^5.1.1",
    "nock": "^8.0.0",
    "openfin-config-builder": "^1.0.8",
    
    "react-addons-test-utils": "^15.2.0",
    "react-hot-loader": "^4.0.1",
    "redux-mock-store": "^1.1.0",
    "remote-redux-devtools": "^0.3.3",
    "remotedev-server": "^0.1.2",
    "rimraf": "^2.5.3",
    "script-loader": "^0.7.0",
    "semver": "^5.1.0",
    "sinon": "^1.17.4",
    "sockjs-client": "^1.1.4",
    "style-loader": "^0.13.1",
    "url-loader": "^1.0.1",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.3"
  },
  "dependencies": {
    "BitFlux": "github:scottlogic/BitFlux#1.7.0",
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^3.3.6",
    "classnames": "^2.2.3",
    "d3fc": "^13.1.1",
    "d3fc-financial-feed": "^3.0.0",
    "d3fc-rebind": "^4.1.1",
    "d3": "^4.4.0",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.1.1",
    "jquery": "^3.5.0",
    "lodash.defaults": "^4.2.0",
    "malihu-custom-scrollbar-plugin": "^3.1.3",
    "moment": "^2.22.0",
    "openfin-launcher": "^1.4.2",
    "prop-types": "^15.6.1",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-redux": "^5.0.7",
    "reduce-reducers": "^0.1.2",
    "redux": "^3.4.0",
    "redux-localstorage": "^0.4.1",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.0.1",
    "reselect": "^2.5.3",
    "seedrandom": "^2.4.2",
    "throat": "^3.0.0"
  }
}
